<?xml version="1.0" encoding="utf-8" ?>

<metal:block
    xmlns="http://www.economics-ejournal.org/namespaces/cover"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal">

    <cover>
 
        <portal_type tal:content="view/portal_type" />
        <id tal:content="context/getId" />
        <date tal:content="view/publish_date" />

        <!-- #TODO f. Journalarticle DOI -->
        <url tal:content="view/uri" />
        <title tal:content="context/Title" />

        <authors>
            <tal:block tal:repeat="author view/authors">
                <author>
                    <name tal:content="author/name" />
                    <affil tal:content="author/affil" />
                </author>
            </tal:block>
        
        </authors>

        <abstract tal:content="structure view/clean_abstract" />

        <jels>
            <tal:block tal:repeat="jel context/jel">
                <jel tal:content="jel" />
            </tal:block>
        </jels>
        
        <tal:block tal:define="ann view/annotations">
            <keywords tal:content="ann/keywords" />    
            <correspondence tal:content="ann/correspondence" />
            <email tal:content="ann/email" />
            <additional tal:content="ann/additional" />
        </tal:block>
        
        <citation tal:content="view/citation_string" />


    </cover>

</metal:block>

